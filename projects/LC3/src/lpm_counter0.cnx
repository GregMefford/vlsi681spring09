GENERATION: STANDARD
VERSION: WM1.0
MODULE: lpm_counter

PRIVATE: ACLR NUMERIC "0"
PRIVATE: ALOAD NUMERIC "0"
PRIVATE: ASET NUMERIC "0"
PRIVATE: ASET_ALL1 NUMERIC "1"
PRIVATE: CLK_EN NUMERIC "0"
PRIVATE: CNT_EN NUMERIC "0"
PRIVATE: CarryIn NUMERIC "0"
PRIVATE: CarryOut NUMERIC "0"
PRIVATE: Direction NUMERIC "0"
PRIVATE: INTENDED_DEVICE_FAMILY STRING "Cyclone"
PRIVATE: ModulusCounter NUMERIC "0"
PRIVATE: ModulusValue NUMERIC "0"
PRIVATE: SCLR NUMERIC "0"
PRIVATE: SLOAD NUMERIC "1"
PRIVATE: SSET NUMERIC "0"
PRIVATE: SSET_ALL1 NUMERIC "1"
PRIVATE: SYNTH_WRAPPER_GEN_POSTFIX STRING "0"
