LIBRARY: lpm lpm.lpm_components.all

GEN_FILE: TYPE_NORMAL lpm_counter0.v TRUE 
GEN_FILE: TYPE_NORMAL lpm_counter0.inc FALSE 
GEN_FILE: TYPE_NORMAL lpm_counter0.cmp FALSE 
GEN_FILE: TYPE_NORMAL lpm_counter0.bsf TRUE FALSE
GEN_FILE: TYPE_NORMAL lpm_counter0_inst.v FALSE 
GEN_FILE: TYPE_NORMAL lpm_counter0_bb.v TRUE 
GEN_FILE: TYPE_NORMAL lpm_counter0_waveforms.html TRUE 
GEN_FILE: TYPE_NORMAL lpm_counter0_wave*.jpg FALSE 

LIB_FILE: lpm

