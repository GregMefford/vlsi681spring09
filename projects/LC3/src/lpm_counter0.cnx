CONSTANT: LPM_DIRECTION STRING "UP" 
CONSTANT: LPM_PORT_UPDOWN STRING "PORT_UNUSED" 
CONSTANT: LPM_TYPE STRING "LPM_COUNTER" 
CONSTANT: LPM_WIDTH NUMERIC "2" 

USED_PORT: clock 0 0 0 0 INPUT NODEFVAL clock 
USED_PORT: data 0 0 2 0 INPUT NODEFVAL data[1..0] 
USED_PORT: q 0 0 2 0 OUTPUT NODEFVAL q[1..0] 
USED_PORT: sload 0 0 0 0 INPUT NODEFVAL sload 

CONNECT: @clock 0 0 0 0 clock 0 0 0 0
CONNECT: q 0 0 2 0 @q 0 0 2 0
CONNECT: @sload 0 0 0 0 sload 0 0 0 0
